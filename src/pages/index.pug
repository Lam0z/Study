block vars
- var lang = "ru";
- var siteTitle = "My Page";


doctype html
html(lang=lang)
	head
		meta(charset="UTF-8")
		meta(http-equiv="X-UA-Compatible", content="IE=edge")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		link(rel="icon", type="image/png", sizes="32x32", href="img/common/fi32x32.png")
		link(rel="icon", type="image/png", sizes="16x16", href="img/common/fi16x16.png")
		link(rel="stylesheet", href="css/libs/swiper.css")
		link(rel="stylesheet", href="css/style.min.css")
		title #{siteTitle}
	body
		.wrapper
			include ../blocks/main/block.pug
					
		script(src="js/libs/jquery.js")
		script(src="js/libs/swiper.js")
		script(src="js/main.min.js")
